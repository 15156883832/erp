package com.jojowonet.modules.fmss.web.util;

import java.util.List;

/**
 * @author Ivan
 */
public class EntityUtils {
	
	/**
	 * 判断List是否为空，如果不为空则返回List中的一个结果
	 * @param list
	 * @return
	 */
	public static <E> E oneEntiryFromList(List<E> list){
		if(list != null && list.size()>0){
			return list.get(0);
		}
		return null;
	}
	
}
