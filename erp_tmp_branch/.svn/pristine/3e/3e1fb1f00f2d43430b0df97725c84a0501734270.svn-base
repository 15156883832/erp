@charset "utf-8";

.navbar .logo{ background-color: #0e8ee7 ;}
/* 导航栏-续费文本 */
.navbar-userbar .btn_renew{color: #0e8ee7 ;}

/*菜单收缩切换*/
.dislpayArrow a{background-position: 5px 0; }
.dislpayArrow a.open{ background-position:0 -49px}
.dislpayArrow a:hover{ text-decoration:none; background-position:-13px 0px}
.dislpayArrow a.open:hover{background-position:-18px -49px}

/*选项卡导航*/
.Hui-tabNav .acrossTab li{ background-position: 0 0;}
.acrossTab li em{background-position: right -30px; }
.acrossTab li:hover,
.acrossTab li.active{background-position:0 -60px;}
.acrossTab li:hover em,
.acrossTab li.active em{background-position:right -90px}

/* sifang icon css */
.sficon{ background-image: url(../images/icons.png) ;}

/* 复选框 */
.label-cbox,
.label-cbox2,
.label-cbox3,
.radiobox,
.label-cbox4{ 
	background-image: url(../images/icons.png) ;	
}

/* 按钮 */
.sfbtn {
	color: #005aab;		
}
.sfbtn-opt2{border: 1px solid #fff;}
.sfbtn-opt,
.sfbtn-opt2:hover,
.btn-count{	
	border: 1px solid #6ac1e4;
	background: -webkit-linear-gradient(top,#e2f0fd,#cce2fa);
	background: -moz-linear-gradient(top,#e2f0fd,#cce2fa);
	background: -ms-linear-gradient(top,#e2f0fd,#cce2fa);
	background: linear-gradient(top,#e2f0fd,#cce2fa);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e2f0fd', endColorstr='#cce2fa',GradientType=0 );
}
.sfbtn-opt3,
.sfbtn-opt:hover,
.sfbtn-active{
	background: #0e8ee7;
	border: 1px solid #0e8ee7;
	color: #fff;
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0e8ee7', endColorstr='#0e8ee7',GradientType=0 );
}
.sfbtn-disabled,
.sfbtn-disabled:hover {
	cursor:not-allowed;
	border: 1px solid #ccc;
	background: #eee;
	color: #888888;
}
/* 发送短信的按钮 */
.sfbtn-green{
	color: #fff;
	background: #0e8ee7;
}
