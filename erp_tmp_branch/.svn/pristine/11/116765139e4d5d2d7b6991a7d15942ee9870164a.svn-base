package com.jojowonet.modules.order.form.vo;

import ivan.common.utils.excel.annotation.ExcelField;

public class AreaManagerVo {
    private String name;
    private String area;
    private String mobile;
    private String code;
    private Long discountAmount;
    private Long siteCount;

    @ExcelField(title = "区管姓名", align = 2, sort=0)
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @ExcelField(title = "区域", align = 2, sort=1)
    public String getArea() {
        return area;
    }

    public void setArea(String area) {
        this.area = area;
    }

    @ExcelField(title = "联系方式", align = 2, sort=2)
    public String getMobile() {
        return mobile;
    }

    public void setMobile(String mobile) {
        this.mobile = mobile;
    }

    @ExcelField(title = "优惠码", align = 2, sort=3)
    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    @ExcelField(title = "优惠额度", align = 2, sort=4)
    public Long getDiscountAmount() {
        return discountAmount;
    }

    public void setDiscountAmount(Long discountAmount) {
        this.discountAmount = discountAmount;
    }

    @ExcelField(title = "服务商个数", align = 2, sort=5)
    public Long getSiteCount() {
        return siteCount;
    }

    public void setSiteCount(Long siteCount) {
        this.siteCount = siteCount;
    }
}
