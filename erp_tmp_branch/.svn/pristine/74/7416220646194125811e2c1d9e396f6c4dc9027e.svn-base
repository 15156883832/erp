function addressChange(pro,city,area){$("#province").val(pro);$.ajax({type:"post",url:ctx+"/order/getCity",async:false,data:{province:pro},dataType:"json",success:function(data){var obj=eval(data);var length=obj.length;if(length<1){}else{var HTML=" ";for(var i=0;i<length;i++){if(obj[i].columns.CityName==city){HTML+='<option value="'+obj[i].columns.CityName+'" selected="selected">'+obj[i].columns.CityName+"</option>"}else{HTML+='<option value="'+obj[i].columns.CityName+'">'+obj[i].columns.CityName+"</option>"}}$("#city").empty();$("#area").empty();$("#city").append(HTML)}},error:function(){return},complete:function(){$.ajax({type:"post",url:ctx+"/order/getArea",async:true,data:{city:city},dataType:"json",success:function(data){var obj=eval(data);var length=obj.length;if(length<1){}else{var HTML=" ";for(var i=0;i<length;i++){if(obj[i].columns.DistrictName==area){HTML+='<option value="'+obj[i].columns.DistrictName+'" selected="selected">'+obj[i].columns.DistrictName+"</option>"}else{HTML+='<option value="'+obj[i].columns.DistrictName+'" >'+obj[i].columns.DistrictName+"</option>"}}$("#area").empty();$("#area").append(HTML)}}});return}})};