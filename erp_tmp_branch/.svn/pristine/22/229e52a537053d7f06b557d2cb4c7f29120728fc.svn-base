package com.jojowonet.modules.order.utils.excelExt.pojo;

public class ExcelItem<T> {

	private ExcelErrorMsg errorMsg;
	private ExcelSuccessMsg successMsg;
	private T item;
	
	public ExcelErrorMsg getErrorMsg() {
		return errorMsg;
	}
	public void setErrorMsg(ExcelErrorMsg errorMsg) {
		this.errorMsg = errorMsg;
	}
	public ExcelSuccessMsg getSuccessMsg() {
		return successMsg;
	}
	public void setSuccessMsg(ExcelSuccessMsg successMsg) {
		this.successMsg = successMsg;
	}
	public T getItem() {
		return item;
	}
	public void setItem(T item) {
		this.item = item;
	}
	
}
