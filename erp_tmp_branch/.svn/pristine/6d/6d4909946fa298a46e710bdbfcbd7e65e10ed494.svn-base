.audioWrap{
    position: absolute;
    top: 24rem/@rem;
    right: 24rem/@rem;
    z-index: 10000;
    width: 75rem/@rem;
    height: 75rem/@rem;
    background: url(../img/music.png) no-repeat;
    background-size: 75rem/@rem 75rem/@rem;
}

.audioPlay{
    -webkit-animation: musicRotate 2s infinite linear;
    animation: musicRotate 2s infinite linear;
}
@keyframes musicRotate{
    0{transform: rotate(0);}
    100%{transform: rotate(360deg);}
}
@-webkit-keyframes musicRotate{
    0{-webkit-transform: rotate(0);}
    100%{-webkit-transform: rotate(360deg);}
}
.btn_nextPage{
    position: absolute;
    bottom: 6rem/@rem;
    left: 50%;
    margin-left: -24rem/@rem;
    width: 48rem/@rem;
    height: 36rem/@rem;
    background: url(../img/bg_next.png) no-repeat;
    background-size: 48rem/@rem 36rem/@rem;
    z-index: 10000;
    -webkit-animation: arrowScale 2s infinite linear;
    animation: arrowScale 2s infinite linear;
}
@keyframes arrowScale{
    0{transform: scale(1);}
    25%{transform: scale(1.2);}
    75%{transform: scale(0.7);}
    100%{transform: scale(1);}
}
@-webkit-keyframes arrowScale{
    0{-webkit-transform: scale(1);}
    25%{-webkit-transform: scale(1.2);}
    75%{-webkit-transform: scale(0.7);}
    100%{-webkit-transform: scale(1);}
}
