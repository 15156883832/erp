@rem:75rem;
/* css reset */
html {
    height: 100%;
    overflow: hidden;
    -webkit-text-size-adjust: 100%!important; // 禁止IOS调整字体大小
    -webkit-user-select: none; // 禁止选中文本
    -webkit-touch-callout: none;
}
body{
    margin:0 auto; 
    font-family:Helvetica,'STHeiti','Microsoft YaHei',Arial,sans-serif; 
    color: #2a2a2a;
    position: relative;
    overflow: hidden;
    height: 100%;
   
}
body *{
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
}
h1,h2,h3,h4,p,ul,li,dl,dd,dt,input,textarea{
    margin:0; 
    padding:0; 
}
ul,li,dl,dd,dt{ list-style:none; }
h1,h2,h3,h4{ font-weight: normal;}


a,a:active,a:hover,a:visited { text-decoration:none;}
 /*去掉标签点击时的蓝色半边框和灰色半透明背景*/ 
a,input,button,optgroup,select,textarea{
    border:none; 
    outline:none;
   background: none;
    -webkit-appearance: none;
    -webkit-tap-highlight-color:rgba(255,0,0,0)
}
a,img{
    -webkit-touch-callout: none;  /* 禁止长按链接与图片弹出菜单 */
}
img{ 
    border:none; 
    vertical-align:top;
}

i,em{ font-style: normal;}
span,a,strong{ display: inline-block;}
body{ max-width: 10rem;}

