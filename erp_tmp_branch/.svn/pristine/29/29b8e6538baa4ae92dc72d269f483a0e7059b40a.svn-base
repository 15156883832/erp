package com.jojowonet.modules.order.form.vo;

import java.util.ArrayList;
import java.util.List;

public class SettlementTemplateVo {
    private List<SettlementItem> items = new ArrayList<>();
    private String name;
    private int index;

    public List<SettlementItem> getItems() {
        return items;
    }

    public void setItems(List<SettlementItem> items) {
        this.items = items;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }


    public int getIndex() {
        return index;
    }

    public void setIndex(int index) {
        this.index = index;
    }
}
